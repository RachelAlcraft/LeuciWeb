@{
    ViewBag.Title = "GenieDisplay";
}

<h2>GenieDisplay</h2>


<canvas id="myCanvas" width="2000" height="1000" style="border:1px solid #c3c3c3;">
Your browser does not support the HTML5 canvas tag.
</canvas>

<script>

var c=document.getElementById("myCanvas");
var ctx=c.getContext("2d");
ctx.fillStyle = "#FF0000";    
ctx.fillRect(0, 0, 150, 80);
ctx.fillRect(200, 0, 150, 80);    
ctx.moveTo(150, 40);
ctx.lineTo(200, 40);
ctx.stroke();

</script>

<div style="width:495px;">        
        <h4>Messages from Calculation</h4>
        <textarea rows="5" cols="10" style="width:450px";>@ViewBag.GenieList</textarea>
        </div>
    <div style="width:495px;"/>


